{
	"compilerOptions": {
		"experimentalDecorators": true,
		"target": "es5",
		"module": "commonjs",
		"sourceMap": true,
		"strict": true,
		"resolveJsonModule": true,
		"esModuleInterop": true,
		"skipLibCheck": true,
		"noImplicitAny": false,
		"forceConsistentCasingInFileNames": true
	},
	"include": [
		"src/*"
	],
	"exclude": [
		"dist"
	]
}